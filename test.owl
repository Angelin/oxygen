<?xml version="1.0"?>
      <!DOCTYPE rdf:RDF [
           <!ENTITY oxy  "http://localhost:8080/#/edit/:domainName#" >
           <!ENTITY owl  "http://www.w3.org/2002/07/owl#" >
           <!ENTITY xsd  "http://www.w3.org/2001/XMLSchema#" >
         ]>

      <rdf:RDF
        xmlns     = "http://localhost:8080/#/edit/:domainName#"
        xmlns:oxy = "http://localhost:8080/#/edit/:domainName#"
        xml:base  = "http://localhost:8080/#/edit/:domainName#"
        xmlns:owl = "http://www.w3.org/2002/07/owl#"
        xmlns:rdf = "http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:rdfs= "http://www.w3.org/2000/01/rdf-schema#"
        xmlns:xsd = "http://www.w3.org/2001/XMLSchema#">

        <owl:Ontology rdf:about="">
          <rdfs:comment>A Knowledge domain based on the semantic search</rdfs:comment>
          <rdfs:label>OXYGEN</rdfs:label>
        </owl:Ontology>

        <owl:Class rdf:ID="Intent" rdf:about="http://localhost:8080/#/edit/:domainName/#intent">
          <rdfs:subClassOf rdf:resource="http://localhost:8080/#/edit/:domainName/#domain"/>
          <rdfs:label xml:lang="en">Intent</rdfs:label>
      	<rdfs:comment xml:lang="en">It is a purpose or indicates the type of knowledge or specific thing one want to learn about or know about</rdfs:comment>
        </owl:Class>


        <owl:Class rdf:ID="Concept" rdf:about="http://localhost:8080/#/edit/:domainName/#concept">
          <rdfs:subClassOf rdf:resource="http://localhost:8080/#/edit/:domainName/#domain"/>
          <rdfs:label xml:lang="en">Concept</rdfs:label>
      	<rdfs:comment xml:lang="en">It is building blocks of Domain Knowledge</rdfs:comment>
        </owl:Class>

        <owl:Class rdf:ID="Term" rdf:about="http://localhost:8080/#/edit/:domainName/#term">
          <rdfs:subClassOf rdf:resource="http://localhost:8080/#/edit/:domainName/#intent"/>
          <rdfs:label xml:lang="en">Term</rdfs:label>
      	<rdfs:comment xml:lang="en">The words which are used in the intents. It can be either indicator or counter-indicator</rdfs:comment>
        </owl:Class>

        <owl:Class rdf:ID="Domain">
          <rdfs:label xml:lang="en">Domain</rdfs:label>
      	<rdfs:comment xml:lang="en">Domain is any topic about which one want to build knowledge base</rdfs:comment>
        </owl:Class>

  </rdf:RDF>
